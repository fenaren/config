                IN      NS      backup1
                IN      NS      backup2
                IN      NS      minecraft
                IN      NS      pi1
                IN      NS      pi2
                IN      NS      router
                IN      NS      sproxy
                IN      NS      tor
                IN      NS      tower
                IN      NS      wap

azel            IN      A       192.168.0.3
backup1         IN      A       192.168.0.10
backup2         IN      A       192.168.0.11
minecraft       IN      A       192.168.0.6
pi1             IN      A       192.168.0.12
pi2             IN      A       192.168.0.13
router          IN      A       192.168.0.1
sproxy          IN      A       192.168.0.4
tor             IN      A       192.168.0.5
tower           IN      A       192.168.0.2
wap             IN      A       192.168.0.14

dhcp            CNAME   backup1
ns              CNAME   backup1
